<%= render 'regular_information', title: "Projects" %>
<% @projects.each do |project| %>
  <%= render partial: "main_information", locals: { project: project } %>
<% end %>
<% if current_user.admin? %>
  <div class="row">
    <div class="col-12">
      <%= link_to 'New Project', new_project_path, class: "btn btn-outline-success btn-block btn-sm" %>
    </div>
  </div>
<% end %>
